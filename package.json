{
  "name": "moltbot-heroku-wrapper",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependencies": [],
    "overrides": {
      "@mariozechner/clipboard": "0.0.0"
    }
  },
  "scripts": {
    "heroku-prebuild": "corepack enable",
    "heroku-postbuild": "pnpm add clawdbot@latest --ignore-scripts --no-optional",
    "start": "bash start.sh"
  }
}
